        <mu1>1000.0</mu1>
        <mu2>1000.0</mu2>
        <xyz>-1000.0  1000.0    1.00</xyz>
        <rpy>    0.0     0.0    0.00</rpy>
        <mesh>default</mesh>
        <size>  1.0    1.0    2.0</size>
        <mass> 10.0</mass>
        <visual>
          <size>1.0    1.0    2.0</size>
          <material>Gazebo/Rocky</material>
          <mesh>unit_box</mesh>
        </visual>
      </geom:box>
    </body:box>
  </model:physical>


  <!-- The large ball map3.png -->
  <model:physical name="ball_model">
    <xyz>   0.0    8.0   300.0</xyz>
    <rpy>   0.0    0.0    0.0</rpy>
    <body:sphere name="ball_body">
      <geom:sphere name="ball_geom">
        <kp>100000.0</kp>
        <kd>1.0</kd>
        <mu1>5.0</mu1>
        <mu2>5.0</mu2>
        <mesh>default</mesh>
        <size> 0.25</size>
        <mass> 10.0</mass>
        <visual>
          <size> 0.5 0.5 0.5</size>
          <material>Gazebo/Rocky</material>
          <mesh>unit_sphere</mesh>
        </visual>
      </geom:sphere>
    </body:sphere>
  </model:physical>



