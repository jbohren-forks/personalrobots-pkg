    <test test-name="2dnav_gazebo_test_2dnav_empty_tmpi"     pkg="2dnav_gazebo" type="set_goal.py" args=" -x 25.70 -y 25.70 -t -3.1416 -nav_t_tol 0.1 -nav_xy_tol 0.20 -odom_t_tol 0.0 -odom_xy_tol 0.0 -timeout 50 " time-limit="60" />

