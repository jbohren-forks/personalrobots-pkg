/**
 * For now, assume a fixed duration, and always return success.
 */
DoorManipulationBehavior::Active{
  meets(Inactive s);
  eq(s.status, SUCCESS);
  defaultOnCommit(duration, 1);
}

// Set the farme id to be odom_bombined for testing purposes
DoorManipulationBehavior::Inactive{
  frame_id == "odom_combined";
}

CheckPath::Active{
  meets(Inactive s);
  eq(s.status, SUCCESS);
  defaultOnCommit(duration, 1);
}

MoveBase::Active{
  meets(Inactive s);
  eq(s.status, SUCCESS);
  defaultOnCommit(duration, 1);
  meets(base_state.Holds bs);
  eq_pose_msg(s.object, object);
  eq_pose_msg(bs.object, object);
}

StopAction::Active{
  defaultOnCommit(duration, 5);
  meets(Inactive s);
  eq(s.status, SUCCESS);
}

CheckPath::Inactive{
  if(status == SUCCESS){
    eq(is_clear, false);
  }
}

UnlatchHandle::Inactive{
  if(status == SUCCESS){
    eq(latch_state, LATCH_STATE_UNLATCHED);
  }
}

PushDoor::Active{
  eq(duration, 10);
}

OpenDoor::Active{
  eq(duration, 10);
}

